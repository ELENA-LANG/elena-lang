import system'routines.
import extensions.

// --- program ---

public program
[
    var Doors := Array new:100; populate(:n)( false ).
    
    0 till:100 do(:i)
    [
        i till:100 by(i + 1) do(:j)
        [  
            Doors[j] := Doors[j] inverted
        ]
    ].
            
    0 till:100 do(:i)
    [
        console printLine("Door #",i + 1," :",Doors[i] iif("Open","Closed"))
    ].
    
    console readChar.
]
