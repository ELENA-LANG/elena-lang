     </primitives>
     <externals>
        <external key="$rt">/usr/lib/elena/libelenart60.so</external>
     </externals>
  </platform>
  <system>
     <platform>69665</platform>
  </system>
  <project>
     <template>lib60</template>
     <prolog>import system;</prolog>
  </project>
  <forwards>
     <forward key="$system_entry">system'core_routines'mta_start</forward>
     <forward key="$symbol_entry">system'$private'entrySymbol</forward>
