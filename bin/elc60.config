        <template key="vm_console">/etc/elena/templates/vm_lnx_console60.config</template>
        <template key="lib60">/etc/elena/templates/lib60.config</template>
     </templates>
     <primitives>
        <primitive key="core">/usr/lib/elena/core/x32/core60_lnx.bin</primitive>
     </primitives>
     <externals>
       <dll key="libc">libc.so.6</dll>
     </externals>
